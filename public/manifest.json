{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "description": "A simple Chrome extension using React and TypeScript with Google Auth",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["identity", "tabs"],
  "background": {
    "service_worker": "static/js/background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "oauth2": {
    "client_id": "235954510797-15nida59jq19c82sjtbajf1gvkc1dmnh.apps.googleusercontent.com",
    "scopes": ["profile", "email"]
  }
}
